code_word = {"Java" : "A strong program language!", "PHP" : "PHP is the best language.", "public" : "公有的", "Apache" : "服务器做得最好的公司", "debug" : "逼格最高"}

code_word["and"] = "和"
code_word["or"] = "或"
code_word["if"] = "如果"
code_word["elif"] = "另外如果"
code_word["else"] = "否则"
for k,v in code_word.items() :
	print(k+","+v)

